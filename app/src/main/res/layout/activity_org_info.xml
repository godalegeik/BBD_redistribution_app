            android:textColor="@color/smokey_black"
            android:textColorHint="@color/mellow_orange"
            app:layout_constraintBottom_toTopOf="@+id/org_info_phone3"
            app:layout_constraintStart_toStartOf="@+id/org_info_phone3"
            app:layout_constraintTop_toBottomOf="@+id/org_info_email3" />

        <EditText
            android:id="@+id/reg_org_adress2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
