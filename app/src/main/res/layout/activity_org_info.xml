            android:textColor="@color/smokey_black"
            android:textColorHint="@color/mellow_orange"
            app:layout_constraintBottom_toTopOf="@+id/reg_org_cancel2"
            app:layout_constraintStart_toStartOf="@+id/reg_org_cancel2"
            app:layout_constraintTop_toBottomOf="@+id/org_info_phone3" />

