            app:layout_constraintEnd_toEndOf="parent" />

        <Button
            android:id="@+id/org_info_password_change"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="16dp"
            android:text="Keisti Slaptazodi"
            android:visibility="invisible"
            app:layout_constraintBottom_toTopOf="@+id/org_info_listView"
            app:layout_constraintEnd_toEndOf="parent" />

        <TextView
            android:id="@+id/org_info_email6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/org_email"
            android:textColor="@color/mango_tango"
            app:layout_constraintBottom_toTopOf="@+id/org_info_email"
            app:layout_constraintStart_toStartOf="@+id/org_info_email"
            app:layout_constraintTop_toBottomOf="@+id/org_info_phone" />

        <TextView
            android:id="@+id/org_info_phone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="org_info_phone"
            android:textColor="@color/smokey_black"
            android:textSize="18sp"
            app:layout_constraintBottom_toTopOf="@+id/org_info_email6"
            app:layout_constraintStart_toStartOf="@+id/org_info_email6"
            app:layout_constraintTop_toBottomOf="@+id/org_info_phone6" />

        <ListView
            android:id="@+id/org_info_listView"
            android:layout_width="407dp"
            android:layout_height="239dp"


            android:background="#7AFFFFFF"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/org_info_type">

        </ListView>

        <TextView
            android:id="@+id/org_info_person"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="org_info_person"
            android:textColor="@color/smokey_black"
            android:textSize="18sp"
            app:layout_constraintBottom_toTopOf="@+id/org_info_phone6"
            app:layout_constraintStart_toStartOf="@+id/org_info_phone6"
            app:layout_constraintTop_toBottomOf="@+id/org_info_person6" />

        <TextView
            android:id="@+id/org_info_email"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
