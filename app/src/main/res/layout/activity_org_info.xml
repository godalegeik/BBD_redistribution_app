            android:textSize="18sp"
            app:layout_constraintBottom_toTopOf="@+id/org_info_type6"
            app:layout_constraintStart_toStartOf="@+id/org_info_type6"
            app:layout_constraintTop_toBottomOf="@+id/org_info_email6" />

        <TextView
            android:id="@+id/org_info_city"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
