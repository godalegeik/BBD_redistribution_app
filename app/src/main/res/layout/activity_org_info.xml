<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    android:backgroundTint="@color/pale_background"
    tools:context=".ui.Orgs.OrgInfoActivity">


    <com.google.android.material.appbar.AppBarLayout
        android:id="@+id/appBarLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:theme="@style/Theme.Redistribution_app.AppBarOverlay"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbar"
            android:layout_width="match_parent"
            android:layout_height="?attr/actionBarSize"
            android:background="?attr/colorPrimary"
            app:popupTheme="@style/Theme.Redistribution_app.PopupOverlay" />
    </com.google.android.material.appbar.AppBarLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/org_info_layout"
        android:layout_width="403dp"
        android:layout_height="667dp"
        android:layout_marginBottom="8dp"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/appBarLayout">

        <TextView
            android:id="@+id/org_info_code"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="org_info_code"
            android:textColor="@color/smokey_black"
            android:textSize="18sp"
            app:layout_constraintBottom_toTopOf="@+id/org_info_adress6"
            app:layout_constraintStart_toStartOf="@+id/org_info_adress6"
            app:layout_constraintTop_toBottomOf="@+id/org_info_code3" />

        <TextView
            android:id="@+id/org_info_code3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
