            android:id="@+id/miestas_spinner2"
            android:layout_width="212dp"
            android:layout_height="35dp"
            app:layout_constraintBottom_toTopOf="@+id/org_info_adress3"
            app:layout_constraintStart_toStartOf="@+id/org_info_adress3"
            app:layout_constraintTop_toBottomOf="@+id/org_info_city3" />

        <Button
            android:id="@+id/reg_org_cancel2"
            android:layout_width="123dp"
            android:layout_height="50dp"
            android:layout_marginStart="28dp"
            android:layout_marginBottom="40dp"
            android:text="AtÅ¡aukti"
