            android:textColor="@color/mango_tango"
            app:layout_constraintBottom_toTopOf="@+id/org_info_type"
            app:layout_constraintStart_toStartOf="@+id/org_info_type"
            app:layout_constraintTop_toBottomOf="@+id/org_info_email" />

        <TextView
            android:id="@+id/org_info_name6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            android:text="@string/org_name"
            android:textColor="@color/mango_tango"
            app:layout_constraintBottom_toTopOf="@+id/org_info_name"
            app:layout_constraintStart_toStartOf="@+id/org_info_name"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/org_info_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="org_info_name"
            android:textColor="@color/smokey_black"
            android:textSize="18sp"
            app:layout_constraintBottom_toTopOf="@+id/org_info_code3"
            app:layout_constraintStart_toStartOf="@+id/org_info_code3"
            app:layout_constraintTop_toBottomOf="@+id/org_info_name6" />

        <TextView
            android:id="@+id/org_info_city6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
