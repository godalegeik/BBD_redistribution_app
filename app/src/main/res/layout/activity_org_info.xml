            android:textColor="@color/smokey_brown"
            app:layout_constraintBottom_toTopOf="@+id/reg_org_person2"
            app:layout_constraintStart_toStartOf="@+id/reg_org_person2"
            app:layout_constraintTop_toBottomOf="@+id/reg_org_code2" />

        <TextView
            android:id="@+id/org_info_city3"
            android:layout_width="wrap_content"
