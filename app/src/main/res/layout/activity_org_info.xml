            android:layout_marginStart="88dp"
            android:layout_marginLeft="88dp"
            android:text="Patvirtinti"
            app:layout_constraintStart_toEndOf="@+id/reg_org_cancel2"
            app:layout_constraintTop_toTopOf="@+id/reg_org_cancel2" />

