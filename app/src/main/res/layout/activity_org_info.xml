            android:textColor="@color/mango_tango"
            app:layout_constraintBottom_toTopOf="@+id/org_info_code"
            app:layout_constraintStart_toStartOf="@+id/org_info_code"
            app:layout_constraintTop_toBottomOf="@+id/org_info_name" />

        <Button
            android:id="@+id/org_info_edit"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="16dp"
