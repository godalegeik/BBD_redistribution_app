        android:layout_height="656dp"
        android:layout_marginStart="5dp"
        android:layout_marginBottom="20dp"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/appBarLayout">

        <EditText
            android:id="@+id/reg_org_code2"
            android:layout_width="293dp"
            android:layout_height="46dp"
            android:ems="10"
            android:hint="@string/org_code"
            android:inputType="number"
            android:textColor="@color/smokey_black"
            android:textColorHint="@color/mellow_orange"
            app:layout_constraintBottom_toTopOf="@+id/org_info_person3"
            app:layout_constraintStart_toStartOf="@+id/org_info_person3"
            app:layout_constraintTop_toBottomOf="@+id/org_info_code2" />

        <EditText
            android:id="@+id/reg_org_name2"
            android:layout_width="236dp"
            android:layout_height="43dp"
            android:ems="10"
            android:hint="@string/org_name"
            android:inputType="textPersonName"
            android:textColor="@color/smokey_black"
            android:textColorHint="@color/mellow_orange"
            app:layout_constraintBottom_toTopOf="@+id/org_info_code2"
            app:layout_constraintStart_toStartOf="@+id/org_info_code2"
            app:layout_constraintTop_toBottomOf="@+id/org_info_name3" />

        <EditText
            android:id="@+id/reg_org_phone2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="10"
            android:hint="@string/org_phone"
